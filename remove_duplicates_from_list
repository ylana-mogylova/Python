
def remove_duplicates(array):
    duplicates = []
    array_without_duplicates = list(array)
    flag = False

    for index, item in enumerate(array):
        print("index=", index, "item=", item)

        for next_index, next_item in enumerate(array[index+1:]):
            print("next_item=%s next_index=%s" % (next_item, next_index))
            if item == next_item:
                print("item=%s, next_item=%s, next_index=%s" %(item, next_item, next_index))
                flag = True
                if item not in duplicates:
                    duplicates.append(item)
                print("array.index(next_item)=", array.index(next_item))
                array_without_duplicates.pop(next_index)
                print("array_without_duplicates=", array_without_duplicates)
    return duplicates, array_without_duplicates


if __name__ == "__main__":
    str_array_input = input('Enter your array:')
    input_str_list = str.split(str_array_input, ",")
    input_list = []
    list_duplicates = []
    no_duplicates = []
    for each in input_str_list:
        input_list.append(int(each.replace(" ", "")))

    print("input array=", input_list)

    list_duplicates, no_duplicates = remove_duplicates(input_list)

    print("list of duplicates = %s, list without duplicates = %s" % (list_duplicates, no_duplicates))
